<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="OurSafetyPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <non-jta-data-source>java:/comp/env/jdbc/oursafetydb</non-jta-data-source>
    <class>domain.Address</class>
    <class>domain.City</class>
    <class>domain.Company</class>
    <class>domain.CompanyNotes</class>
    <class>domain.CompanyPerson</class>
    <class>domain.CompanyPersonAddress</class>
    <class>domain.CompanyPersonPhone</class>
    <class>domain.CompanyPositions</class>
    <class>domain.CompanyRelationship</class>
    <class>domain.CompanyType</class>
    <class>domain.Country</class>
    <class>domain.EmergencyContact</class>
    <class>domain.Item</class>
    <class>domain.ItemClass</class>
    <class>domain.ItemClassFields</class>
    <class>domain.Logins</class>
    <class>domain.Manual</class>
    <class>domain.ManualUse</class>
    <class>domain.Person</class>
    <class>domain.Phone</class>
    <class>domain.Province</class>
    <class>domain.TypeLibrary</class>
    <class>domain.Url</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
  </persistence-unit>
</persistence>
